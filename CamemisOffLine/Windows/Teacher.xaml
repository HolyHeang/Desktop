<Window x:Class="CamemisOffLine.Teacher"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:CamemisOffLine"
        mc:Ignorable="d"
       xmlns:language="clr-namespace:CamemisOffLine.Properties.Langs"
        xmlns:properties="clr-namespace:CamemisOffLine.Properties"
        xmlns:class="clr-namespace:CamemisOffLine.Class"
        Title="CAMEMIS" Height="1080" Width="1920" WindowStartupLocation="CenterScreen" WindowState="Maximized" Loaded="Window_Loaded" WindowStyle="None">
    <Grid>

        <DockPanel LastChildFill="True" Grid.ColumnSpan="2" Margin="0,0,-0.4,-0.4">
            <DockPanel DockPanel.Dock="Left" Height="auto" Width="320" Name="slideLeft" MouseEnter="slideLeft_MouseEnter" MouseLeave="slideLeft_MouseLeave" LastChildFill="True">
                <DockPanel.Background>
                    <SolidColorBrush Color="#1183CA"/>
                </DockPanel.Background>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="35"></RowDefinition>
                        <RowDefinition Height="76"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="35"></RowDefinition>
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="60"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>

                        <Grid Grid.Column="0">

                            <!--<Grid Name="gridMenu" VerticalAlignment="Center" HorizontalAlignment="Center">
                                <materialDesign:PackIcon Kind="Menu" Foreground="White" Height="35" Width="35" Cursor="Hand"  MouseDown="gridMenu_MouseDown" ToolTip="{x:Static language:Lang.Tooltip_top_bar_Menu}" HorizontalAlignment="Right" Margin="-11,0,2,0"/>
                            </Grid>-->

                        </Grid>
                        <Grid Grid.Column="1">
                            <Grid DataContext="{x:Static properties:Settings.Default}">
                                <TextBlock Text="{Binding schoolName}" Name="TitleSchool" Style="{DynamicResource Muol}" FontSize="12" Foreground="White" HorizontalAlignment="Left" Margin="10 0 0 0" VerticalAlignment="Center"></TextBlock>
                            </Grid>
                        </Grid>
                    </Grid>
                    <Grid x:Name="gridAcc" Margin="0 0 0 0" Grid.Row="1" Background="#1183CA" DataContext="{x:Static properties:Settings.Default}">
                        <Grid Margin="5 0 5 5" Background="#66111111">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="2.8*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="0">
                                    <Grid>
                                        <Image Name="imgUserProfile" Source="/CamemisOffLine;component/Image/account (1).png" Cursor="Hand" MouseDown="Image_MouseDown" Width="60" Height="60"></Image>
                                    </Grid>
                                </Grid>
                                <Grid Grid.Column="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1.3*"></RowDefinition>
                                        <RowDefinition Height="*"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0">
                                        <TextBlock Text="{Binding profileName}" Margin="10 10 0 0" VerticalAlignment="Center" HorizontalAlignment="Left" FontSize="18" FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Battambang" Foreground="White"></TextBlock>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <Label Name="lblForm" Margin="5 0 0 0" Content="{x:Static language:Lang.Admin_Menu}" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="White" FontSize="12" FontWeight="Bold" Style="{DynamicResource Siemreap}"></Label>

                                    </Grid>
                                </Grid>
                            </Grid>
                        </Grid>

                    </Grid>

                    <ScrollViewer Grid.Row="2" VerticalScrollBarVisibility="Auto">
                        <Grid Name="gridfeature">
                            <StackPanel Margin="0,0,0,0">


                                <StackPanel Orientation="Vertical" Name="adminPart" HorizontalAlignment="Stretch"  Height="auto" >
                                    <Button Name="btnStaffAttendanceReport" materialDesign:ShadowAssist.ShadowDepth="Depth0" Height="35" HorizontalAlignment="Stretch" Background="Transparent" BorderThickness="0" Margin="5,10,5,5" Click="btnStaffAttendanceReport_Click" MouseEnter="btnStaffAttendanceReport_MouseEnter" MouseLeave="btnStaffAttendanceReport_MouseLeave">

                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">

                                            <materialDesign:PackIcon Kind="ChartBoxOutline" Name="MateriaStaffAtt" VerticalAlignment="Center" Foreground="White" HorizontalAlignment="Left" Height="24" Width="24" Margin="0,0,0,0" ></materialDesign:PackIcon>
                                            <Label Name="lblStaffAtt" Content="{x:Static language:Lang.Staff_Att_Re}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="34" Width="240" Foreground="White" Margin="0,-4,1,0" Style="{DynamicResource Siemreap}" FontWeight="Bold"/>
                                        </Grid>
                                    </Button>
                                    <Button Name="btnStudentAttendanceReport" materialDesign:ShadowAssist.ShadowDepth="Depth0" Height="35" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" Margin="5,-3,5,5" Click="btnStudentAttendanceReport_Click_1" MouseEnter="btnStudentAttendanceReport_MouseEnter_1" MouseLeave="btnStudentAttendanceReport_MouseLeave_1">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">

                                            <materialDesign:PackIcon Name="MateriaStuAtt" Foreground="White" Kind="TextBoxCheckOutline" VerticalAlignment="Center" HorizontalAlignment="Left" Height="24" Width="24" Margin="0,0,0,0"></materialDesign:PackIcon>
                                            <Label Name="lblStuAtt" FontWeight="Bold" Content="{x:Static language:Lang.Stu_Att_Re}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="34" Width="240" Foreground="White" Margin="0,-4,1,0" Style="{DynamicResource Siemreap}"/>
                                        </Grid>
                                    </Button>
                                    <Button Name="btnStudentLearningRsult" materialDesign:ShadowAssist.ShadowDepth="Depth0" Height="35" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="5,-3,5,5" Click="btnStudentLearningRsult_Click" MouseEnter="btnStudentLearningRsult_MouseEnter" MouseLeave="btnStudentLearningRsult_MouseLeave">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">
                                            <materialDesign:PackIcon Name="MateriaStuLearn" Foreground="White" Kind="ClipboardTextOutline" VerticalAlignment="Center" HorizontalAlignment="Left" Height="24" Width="24" Margin="0,0,0,0" ></materialDesign:PackIcon>
                                            <Label Name="lblStuLearn" FontWeight="Bold" Content="{x:Static language:Lang.Stu_Learn_Result}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="34" Width="244" Style="{DynamicResource Siemreap}" Foreground="White" Margin="0,0,-4,0"></Label>
                                        </Grid>
                                    </Button>

                                    <Button Name="btnClassSchedule" Height="35" materialDesign:ShadowAssist.ShadowDepth="Depth0" HorizontalAlignment="Stretch" Background="Transparent" BorderThickness="0" FontSize="15" Margin="5,-3,5,5" Click="btnClassSchedule_Click" MouseEnter="btnClassSchedule_MouseEnter" MouseLeave="btnClassSchedule_MouseLeave">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">
                                            <materialDesign:PackIcon Name="MateriaClassSche" Kind="CalendarClock" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Left" Height="24" Width="24" Margin="0,0,0,0" ></materialDesign:PackIcon>
                                            <Label Name="lblClassSched" FontWeight="Bold" Content="{x:Static language:Lang.Cl_Sch}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="34" Width="247" Foreground="White" Margin="0,-3,-6,-1" Style="{DynamicResource Siemreap}"></Label>
                                        </Grid>
                                    </Button>
                                    <Button Name="btnTeacherSchedule" Height="35" materialDesign:ShadowAssist.ShadowDepth="Depth0" HorizontalAlignment="Stretch" Background="Transparent" BorderThickness="0" FontSize="15" Margin="5,-3,5,5" Click="btnTeacherSchedule_Click" MouseEnter="btnTeacherSchedule_MouseEnter" MouseLeave="btnTeacherSchedule_MouseLeave">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">

                                            <materialDesign:PackIcon Name="MateriaTeacherSche" Foreground="White" Kind="CalendarRangeOutline" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="0,0,0,0" Height="24" Width="24" ></materialDesign:PackIcon>
                                            <Label Name="lblTeacherSche" FontWeight="Bold" Content="{x:Static language:Lang.Tea_Sche}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="36" Width="247" Foreground="White" Margin="0,-6,-5,0" Style="{DynamicResource Siemreap}"/>
                                        </Grid>
                                    </Button>
                                    <Button Name="btnStudentList" Height="35" materialDesign:ShadowAssist.ShadowDepth="Depth0" HorizontalAlignment="Stretch" Background="Transparent" BorderThickness="0" FontSize="15" Margin="5,-3,5,5" Click="btnStudentList_Click" MouseEnter="btnStudentList_MouseEnter" MouseLeave="btnStudentList_MouseLeave">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">
                                            <materialDesign:PackIcon Name="MateriaStu" Kind="TextBoxOutline" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Left" Height="24" Margin="0,0,0,0" Width="24" ></materialDesign:PackIcon>
                                            <Label Name="lblStuList" FontWeight="Bold" Content="{x:Static language:Lang.Stu_List}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="35" Width="245" Foreground="White" Margin="0,-3,-5,-2" Style="{DynamicResource Siemreap}"></Label>
                                        </Grid>
                                    </Button>
                                    <Button Name="btnSubJectList" Height="35" Click="btnSubJectList_Click" materialDesign:ShadowAssist.ShadowDepth="Depth0" HorizontalAlignment="Stretch" Background="Transparent" BorderThickness="0" FontSize="15" Margin="5,-3,5,5" MouseEnter="btnSubJectList_MouseEnter_1" MouseLeave="btnSubJectList_MouseLeave">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">
                                            <materialDesign:PackIcon Name="MateriaSubj" Kind="ViewListOutline" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Left" Height="24" Margin="0,0,0,0" Width="24" ></materialDesign:PackIcon>
                                            <Label Name="lblSubject" FontWeight="Bold" Content="{x:Static language:Lang.Sub_List}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="33" Width="246" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                        </Grid>
                                    </Button>

                                </StackPanel>
                                <StackPanel Name="TeacherPart" Orientation="Vertical" VerticalAlignment="Stretch" Margin="-5,0,0,0"/>

                                <StackPanel Name="SettingPart" Margin="0 50 0 0">
                                    <Separator Foreground="White" VerticalAlignment="Bottom"/>
                                    <Button Name="btnFeaPrint" Height="35" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="5,0,5,5" Click="btnFeaPrint_Click">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">
                                            <materialDesign:PackIcon Name="MateriaFeaprint" Kind="printer" VerticalAlignment="Center" HorizontalAlignment="Left" Height="24" Margin="-2,0,0,0" Width="28" ></materialDesign:PackIcon>
                                            <Label Name="lblFeaprint" FontWeight="Bold" Content="{x:Static language:Lang.print}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="33" Width="246" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                            <materialDesign:PackIcon Name="MateriaFeaprintdrop" Kind="ChevronDown" VerticalAlignment="Center" HorizontalAlignment="Right" Height="24" Margin="0,0,15,0" Width="28" ></materialDesign:PackIcon>
                                            <materialDesign:PackIcon Name="MateriaFeaprintUp" Kind="ChevronUp" VerticalAlignment="Center" HorizontalAlignment="Right" Height="24" Margin="0,0,15,0" Width="28" ></materialDesign:PackIcon>

                                        </Grid>
                                    </Button>
                                    <Button Name="btnStudent" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="50,-5,5,10" Click="btnStudent_Click">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="222" Height="25">
                                            <materialDesign:PackIcon Kind="Printer" VerticalAlignment="Center" HorizontalAlignment="Left" Height="20" Margin="0,0,0,0" Width="24" ></materialDesign:PackIcon>
                                            <Label FontWeight="Bold" Content="{x:Static language:Lang.staffacademy}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="12" VerticalContentAlignment="Center" Height="33" Width="205" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                        </Grid>
                                    </Button>
                                    <Button Name="btnTeacher" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="50,-5,5,10" Click="btnTeacher_Click">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="222" Height="25">
                                            <materialDesign:PackIcon Kind="Printer" VerticalAlignment="Center" HorizontalAlignment="Left" Height="20" Margin="0,0,0,0" Width="24" ></materialDesign:PackIcon>
                                            <Label FontWeight="Bold" Content="{x:Static language:Lang.staffadmin}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="12" VerticalContentAlignment="Center" Height="33" Width="205" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                        </Grid>
                                    </Button>

                                    <Separator Foreground="Gray" VerticalAlignment="Bottom"></Separator>

                                    <Button Name="btnSetting" Height="35" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="5,0,5,5" Click="btnSetting_Click">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">
                                            <materialDesign:PackIcon Name="MateriaSetting" Kind="Settings" VerticalAlignment="Center" HorizontalAlignment="Left" Height="24" Margin="-2,0,0,0" Width="28" ></materialDesign:PackIcon>
                                            <Label Name="lblSetting" FontWeight="Bold" Content="{x:Static language:Lang.setting}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="33" Width="246" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                            <materialDesign:PackIcon Name="MateriaSettingDrop" Kind="ChevronDown" VerticalAlignment="Center" HorizontalAlignment="Right" Height="24" Margin="0,0,15,0" Width="28" ></materialDesign:PackIcon>
                                            <materialDesign:PackIcon Name="MateriaSettingUp" Kind="ChevronUp" VerticalAlignment="Center" HorizontalAlignment="Right" Height="24" Margin="0,0,15,0" Width="28" ></materialDesign:PackIcon>

                                        </Grid>
                                    </Button>

                                    <Button Name="btnLang" Click="btnLang_Click" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="50,0,5,10">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="222" Height="25">
                                            <materialDesign:PackIcon Name="MateriaLang" Kind="Language" VerticalAlignment="Center" HorizontalAlignment="Left" Height="20" Margin="0,0,0,0" Width="24" ></materialDesign:PackIcon>
                                            <Label Name="lblLang" FontWeight="Bold" Content="{x:Static language:Lang.language}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="12" VerticalContentAlignment="Center" Height="33" Width="205" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                            <materialDesign:PackIcon Name="MateriaLangDrop" Kind="ChevronDown" VerticalAlignment="Center" HorizontalAlignment="Right" Height="20" Margin="0,0,27,0" Width="24" ></materialDesign:PackIcon>
                                            <materialDesign:PackIcon Name="MateriaLangUp" Kind="ChevronUp" VerticalAlignment="Center" HorizontalAlignment="Right" Height="20" Margin="0,0,27,0" Width="24" ></materialDesign:PackIcon>

                                        </Grid>
                                    </Button>

                                    <Grid Height="30" Margin="85 -5 40 0" Name="gridLanguage">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="39"></ColumnDefinition>
                                            <ColumnDefinition Width="39"></ColumnDefinition>
                                            <ColumnDefinition Width="39"></ColumnDefinition>
                                            <ColumnDefinition Width="39"></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid Grid.Column="0">
                                            <Button x:Name="Khmer" ToolTip="ខ្មែរ" Click="Khmer_Click_1" Height="30" HorizontalAlignment="Stretch" Background="Transparent" BorderThickness="0" FontSize="15">
                                                <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10" Height="30">
                                                    <Image  Name="Langkh" Source="/CamemisOffLine;component/Image/cambodia.png" Stretch="Fill" HorizontalAlignment="Left" VerticalAlignment="Center" Height="25" Width="27" ></Image>

                                                </Grid>
                                            </Button>
                                        </Grid>
                                        <Grid Grid.Column="1">
                                            <Button x:Name="English" ToolTip="English" Click="English_Click_1" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15">
                                                <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10" Height="30">
                                                    <Image  Name="LangEng" Source="/CamemisOffLine;component/Image/united-kingdom.png" Stretch="Fill" HorizontalAlignment="Left" VerticalAlignment="Center" Height="25" Width="27" ></Image>
                                                </Grid>
                                            </Button>
                                        </Grid>
                                        <Grid Grid.Column="2">
                                            <Button Name="Chinese" ToolTip="Chinese" Click="Chinese_Click_1" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15">
                                                <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin=" -10 -12 -10 -10 " Height="25">
                                                    <Image  Name="LangChin" Source="/CamemisOffLine;component/Image/chinese.png" Stretch="Fill" HorizontalAlignment="Left" VerticalAlignment="Center" Height="17" Width="28" ></Image>
                                                </Grid>
                                            </Button>
                                        </Grid>
                                        <Grid Grid.Column="3">
                                            <Button x:Name="Vietnam" ToolTip="Veitnamese" Click="Vietnam_Click_1" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15">
                                                <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10" Height="25">
                                                    <Image  Name="LangVit" Source="/CamemisOffLine;component/Image/vietnam.png" Stretch="Fill" HorizontalAlignment="Left" VerticalAlignment="Center" Height="25" Width="27" ></Image>

                                                </Grid>
                                            </Button>
                                        </Grid>
                                    </Grid>





                                    <Button Name="btnColor" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="50,0,5,10">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="222" Height="25">
                                            <materialDesign:PackIcon Name="MateriaColor" Kind="Color" VerticalAlignment="Center" HorizontalAlignment="Left" Height="20" Margin="0,0,0,0" Width="24" ></materialDesign:PackIcon>
                                            <Label Name="lblColor" FontWeight="Bold" Content="{x:Static language:Lang.color}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="12" VerticalContentAlignment="Center" Height="33" Width="205" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                            <materialDesign:PackIcon Name="MateriaColorDrop" Kind="ChevronDown" VerticalAlignment="Center" HorizontalAlignment="Right" Height="20" Margin="0,0,27,0" Width="24" ></materialDesign:PackIcon>
                                            <materialDesign:PackIcon Name="MateriaColorUp" Kind="ChevronUp" VerticalAlignment="Center" HorizontalAlignment="Right" Height="20" Margin="0,0,27,0" Width="24" ></materialDesign:PackIcon>

                                        </Grid>
                                    </Button>
                                    <Button Name="btnAbout" Click="btnAbout_Click" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="50,0,5,10">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="222" Height="25">
                                            <materialDesign:PackIcon Name="MateriaAbout" Kind="AboutOutline" VerticalAlignment="Center" HorizontalAlignment="Left" Height="20" Margin="0,0,0,0" Width="24" ></materialDesign:PackIcon>
                                            <Label Name="lblAbout" FontWeight="Bold" Content="{x:Static language:Lang.about}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="12" VerticalContentAlignment="Center" Height="33" Width="205" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                        </Grid>
                                    </Button>
                                    <Separator Foreground="Gray" VerticalAlignment="Bottom"></Separator>

                                    <Button Name="btnLogout" Height="35" Click="btnLogout_Click" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="5,0,5,5">
                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">
                                            <materialDesign:PackIcon Name="MateriaLogout" Kind="Logout" VerticalAlignment="Center" HorizontalAlignment="Left" Height="24" Margin="-2,0,0,0" Width="28" ></materialDesign:PackIcon>
                                            <Label Name="lblLogout" FontWeight="Bold" Content="{x:Static language:Lang.logout}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="33" Width="246" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                        </Grid>
                                    </Button>
                                    <Separator Foreground="Gray" VerticalAlignment="Bottom"></Separator>

                                </StackPanel>

                            </StackPanel>


                        </Grid>
                    </ScrollViewer>
                    <StackPanel Grid.Row="3" Margin="0 0 0 0">
                        <Grid>
                            <Label Name="lblnameCompany" Content="Power by CAMEMIS" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="15" FontFamily="Segoe UI Semibold"  Foreground="White">

                            </Label>
                        </Grid>
                    </StackPanel>
                </Grid>
            </DockPanel>
            <DockPanel DockPanel.Dock="Right" Margin="0,0,0,0.4" MouseLeftButtonDown="DockPanel_MouseLeftButtonDown">
                <DockPanel DockPanel.Dock="Top" HorizontalAlignment="Stretch" Background="White" Height="40" VerticalAlignment="Top">

                    <Grid Margin="0 0 0 5">
                        <Border BorderBrush="Gray" BorderThickness="0 0 0 1"></Border>
                        <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                            <Label Name="Tiltle" FontSize="15" FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Battambang" FontWeight="Bold"></Label>
                        </Grid>
                        <Grid>
                            <materialDesign:PackIcon Name="wifiIcon" Kind="Wifi" Foreground="Green" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0 0 330 0"/>
                            <TextBlock Name="txtPing" Text="Ping: 22ms" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0 0 260 0" FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Battambang"/>
                            <Grid HorizontalAlignment="Right" Width="150" Height="40" Margin="0,0,102,0" VerticalAlignment="Center">
                                <Label Name="txtxCheckinternet" HorizontalAlignment="Left" Content="Online"  VerticalContentAlignment="Center" HorizontalContentAlignment="Right" Width="80" FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Battambang"></Label>
                                <ToggleButton Name="btnCheck" Foreground="Black" TextBlock.FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Battambang" Background="Blue" Height="20" Width="70" HorizontalAlignment="Right" VerticalAlignment="Center" Click="btnCheck_Click">
                                    <materialDesign:ToggleButtonAssist.SwitchTrackOffBackground>
                                        <SolidColorBrush Color="Black" TextOptions.TextHintingMode="Animated"></SolidColorBrush>
                                    </materialDesign:ToggleButtonAssist.SwitchTrackOffBackground>
                                    <materialDesign:ToggleButtonAssist.SwitchTrackOnBackground>
                                        <SolidColorBrush Color="Blue" TextOptions.TextHintingMode="Animated"></SolidColorBrush>
                                    </materialDesign:ToggleButtonAssist.SwitchTrackOnBackground>
                                </ToggleButton>
                            </Grid>
                            <Grid Name="gridmax" Width="35" HorizontalAlignment="Right" Height="35" Cursor="Hand" ToolTip="{x:Static language:Lang.Tooltip_top_bar_Max}" MouseDown="gridmax_MouseDown" MouseEnter="gridmini_MouseEnter" MouseLeave="gridmini_MouseLeave" Margin="0,0,35,0">
                                <materialDesign:PackIcon Kind="WindowMaximize" FontSize="15" Height="20" Width="20" Foreground="Gray" Margin="7,8,0,0"/>
                            </Grid>
                            <Grid x:Name="gridmini" Width="35" Height="35" Cursor="Hand" HorizontalAlignment="Right" ToolTip="{x:Static language:Lang.Tooltip_top_bar_Min}" MouseDown="gridmini_MouseDown" MouseEnter="gridmini_MouseEnter" MouseLeave="gridmini_MouseLeave" Margin="0,0,70,0">
                                <materialDesign:PackIcon Kind="WindowMinimize" FontSize="15" Height="20" Width="20"  Foreground="Gray" Margin="8,8,0,0"/>
                            </Grid>
                            <Grid Name="gridExit" Width="35" Cursor="Hand" Height="35"  HorizontalAlignment="Right" ToolTip="{x:Static language:Lang.Tooltip_top_bar_Close}" MouseDown="gridExit_MouseDown" MouseEnter="gridMenu_MouseEnter" MouseLeave="gridMenu_MouseLeave">
                                <materialDesign:PackIcon Kind="Close" FontSize="15" Height="20" Width="20" Foreground="Gray" VerticalAlignment="Stretch" Margin="8,8,0,7"/>
                            </Grid>
                        </Grid>
                    </Grid>
                </DockPanel>
                <DockPanel Margin="0">
                    <Grid>
                        <TabControl Name="tabMenu" BorderBrush="White" Background="White" RenderTransformOrigin="0.501,0.404" Margin="0,-4,-0.4,2" Grid.ColumnSpan="2">

                            <TabItem Name="HomePage">
                                <StackPanel Margin="150,0,150,126" HorizontalAlignment="Center" Width="516" Height="388" VerticalAlignment="Center">
                                    <Image Source="/CamemisOffLine;component/Image/camemisBlue.png" Margin="64,0,55,0" Height="224">
