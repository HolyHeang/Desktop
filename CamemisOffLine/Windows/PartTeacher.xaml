                                                                        <ColumnDefinition Width="*"/>
                                                                        <ColumnDefinition Width="*"/>
                                                                        <ColumnDefinition Width="*"/>
                                                                    </Grid.ColumnDefinitions>

                                                                    

                                                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Width="auto" Height="40" Margin="0,-2,10,-3" VerticalAlignment="Center" Grid.Column="2">

