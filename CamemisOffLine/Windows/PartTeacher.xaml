                                                                                <Button Name="btnInputScore" FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Muol Light" FontSize="12" Foreground="Black" Content="{x:Static language:Lang.inputscore}" Width="120" Background="#66D3D3D3" BorderThickness="0 0 1 0" BorderBrush="WhiteSmoke" Click="btnInputScore_Click" Height="Auto" Margin="0,0,0,-2" materialDesign:ShadowAssist.ShadowDepth="Depth0"/>

                                                                            </Grid>
                                                                            <Grid>
