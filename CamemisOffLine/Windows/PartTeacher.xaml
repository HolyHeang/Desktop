                                                            <TreeView Name="tvAcademy" Margin="-35 -10 0 0" BorderThickness="0" FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Battambang" TreeView.SelectedItemChanged="tvAcademy_SelectedItemChanged">
                                                                <TreeView.Resources>

                                                                    <Style TargetType="TreeViewItem">
                                                                        <Setter Property="IsExpanded" Value="True">

                                                                        </Setter>
