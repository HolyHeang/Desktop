                                                                        <HierarchicalDataTemplate.ItemContainerStyle>
                                                                            <Style TargetType="TreeViewItem">
                                                                                <Setter Property="IsExpanded" Value="{Binding IsExpande}"/>
                                                                            </Style>
                                                                        </HierarchicalDataTemplate.ItemContainerStyle>
                                                                        --><!--<StackPanel Orientation="Horizontal">
                                                                            <TextBlock Text="{Binding name}" FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Battambang" Foreground="Blue" FontSize="12"></TextBlock>
                                                                        </StackPanel>--><!--
                                                                    </HierarchicalDataTemplate>
                                                                    <HierarchicalDataTemplate DataType="{x:Type local:TeachingClass}">
                                                                        <HierarchicalDataTemplate.ItemContainerStyle>
                                                                            <Style TargetType="TreeViewItem">
                                                                                <Setter Property="IsExpanded" Value="{Binding IsExpande}"/>
                                                                            </Style>
                                                                        </HierarchicalDataTemplate.ItemContainerStyle>
