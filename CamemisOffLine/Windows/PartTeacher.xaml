<Window x:Class="CamemisOffLine.Windows.PartTeacher"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library;assembly=Library"
         xmlns:properties="clr-namespace:CamemisOffLine.Properties"
         xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"
         xmlns:language="clr-namespace:CamemisOffLine.Properties.Langs"
        Title="PartTeacher" Height="1080" Width="1920"  AllowsTransparency="False" ResizeMode="CanResizeWithGrip" Loaded="Window_Loaded" WindowState="Maximized" WindowStyle="None">
    <Grid Name="GridForm">
        <Grid.RowDefinitions>
            <RowDefinition Height="35"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0" >

            <Grid HorizontalAlignment="Left" Margin="20 0 0 0">
                <Grid.ColumnDefinitions>

                    <ColumnDefinition Width="*"></ColumnDefinition>
                </Grid.ColumnDefinitions>

                <Grid Grid.Column="0">

                    <!--<Grid Name="gridMenu" VerticalAlignment="Center" HorizontalAlignment="Center">
                                <materialDesign:PackIcon Kind="Menu" Foreground="White" Height="35" Width="35" Cursor="Hand"  MouseDown="gridMenu_MouseDown" ToolTip="{x:Static language:Lang.Tooltip_top_bar_Menu}" HorizontalAlignment="Right" Margin="-11,0,2,0"/>
                            </Grid>-->

                </Grid>
                <Grid Grid.Column="0" Name="titleSchool">
                    <Grid>
                        <TextBlock Text="បុរី១០០ខ្នង" Name="TitleSchool" Style="{DynamicResource Muol}" FontSize="12" Foreground="Black" HorizontalAlignment="Center" Margin="0 0 0 0" VerticalAlignment="Center"></TextBlock>
                    </Grid>
                </Grid>
            </Grid>
            <Grid  VerticalAlignment="Center" MouseLeftButtonDown="Grid_MouseLeftButtonDown">
                <Label Name="lblTitle" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Title" FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Muol Light" FontSize="15"></Label>
            </Grid>
            <Border BorderBrush="Black" BorderThickness="0 0 0 1"></Border>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                <materialDesign:PackIcon Name="wifiIcon" Kind="Wifi" Foreground="Green" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0 0 10 0"/>
                <TextBlock Name="txtPing" Text="Ping: 22ms" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0 0 0 0" FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Battambang"/>
                <Grid HorizontalAlignment="Right" Width="150" Height="40" Margin="0,0,0,0" VerticalAlignment="Center">
                    <Label Name="txtxCheckinternet" HorizontalAlignment="Left" Content="Online"  VerticalContentAlignment="Center" HorizontalContentAlignment="Right" Width="80" FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Battambang"></Label>
                    <ToggleButton Name="btnCheck" Foreground="Black" TextBlock.FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Battambang" Background="Blue" Height="20" Width="70" HorizontalAlignment="Right" VerticalAlignment="Center" Click="btnCheck_Click">
                        <materialDesign:ToggleButtonAssist.SwitchTrackOffBackground>
                            <SolidColorBrush Color="Black" TextOptions.TextHintingMode="Animated"></SolidColorBrush>
                        </materialDesign:ToggleButtonAssist.SwitchTrackOffBackground>
                        <materialDesign:ToggleButtonAssist.SwitchTrackOnBackground>
                            <SolidColorBrush Color="Blue" TextOptions.TextHintingMode="Animated"></SolidColorBrush>
                        </materialDesign:ToggleButtonAssist.SwitchTrackOnBackground>
                    </ToggleButton>
                </Grid>
                <Grid Name="gridMin" HorizontalAlignment="Center" Height="35" Width="35" Cursor="Hand" MouseDown="gridMin_MouseDown" MouseEnter="gridMin_MouseEnter" MouseLeave="gridMin_MouseLeave">
                    <materialDesign:PackIcon Name="materiaMin" VerticalAlignment="Center" HorizontalAlignment="Center" Kind="WindowMinimize" Height="15" Width="15"/>
                </Grid>
                <Grid Name="gridMax" HorizontalAlignment="Center" Height="35" Width="35" Cursor="Hand" MouseDown="gridMax_MouseDown" MouseEnter="gridMax_MouseEnter" MouseLeave="gridMax_MouseLeave">
                    <materialDesign:PackIcon Name="materiaMax" VerticalAlignment="Center" HorizontalAlignment="Center" Kind="WindowRestore" Height="15" Width="15"/>
                </Grid>
                <Grid Name="gridClose" HorizontalAlignment="Center" Height="35" Cursor="Hand" Width="35" MouseDown="gridClose_MouseDown" MouseEnter="gridClose_MouseEnter" MouseLeave="gridClose_MouseLeave">
                    <materialDesign:PackIcon Name="materiaExit" VerticalAlignment="Center" HorizontalAlignment="Center" Kind="WindowClose" Height="15" Width="15"/>
                </Grid>
            </StackPanel>
        </Grid>


        <DockPanel LastChildFill="True" Grid.Row="1">

            <DockPanel DockPanel.Dock="Left" Background="#1183CA" Width="320" Height="auto" Name="slidLeft" MouseEnter="slidLeft_MouseEnter" MouseLeave="slidLeft_MouseLeave">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="75"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="35"></RowDefinition>
                    </Grid.RowDefinitions>


                    <Grid x:Name="gridAcc" Margin="0 0 0 0" Grid.Row="0" Background="#1183CA" DataContext="{x:Static properties:Settings.Default}">
                        <Grid Margin="5 5 5 5" Background="#66111111">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="2.8*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="0">
                                    <Grid>
                                        <Image Name="imgUserProfile" Source="/CamemisOffLine;component/Image/account (1).png" Cursor="Hand" Width="60" Height="60"></Image>
                                    </Grid>
                                </Grid>
                                <Grid Grid.Column="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1.3*"></RowDefinition>
                                        <RowDefinition Height="*"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0">
                                        <TextBlock Text="{Binding profileName}" Margin="10 10 0 0" VerticalAlignment="Center" HorizontalAlignment="Left" FontSize="18" FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Battambang" Foreground="White"></TextBlock>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <Label Name="lblForm" Margin="5 0 0 0" Content="Teacher" VerticalAlignment="Center" HorizontalAlignment="Left" Foreground="White" FontSize="12" FontWeight="Bold" Style="{DynamicResource Siemreap}"></Label>

                                    </Grid>
                                </Grid>
                            </Grid>
                        </Grid>

                    </Grid>

                    <ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Auto">
                        <Grid Name="gridfeature">
                            <StackPanel Margin="0,0,0,0">


                                <StackPanel Orientation="Vertical" Name="TeacherPart" HorizontalAlignment="Stretch"  Height="auto" >
                                    <Button Name="btnScore" materialDesign:ShadowAssist.ShadowDepth="Depth0" Height="35" HorizontalAlignment="Stretch" Background="Transparent" BorderThickness="0" Margin="5,10,5,5" Click="btnScore_Click" MouseEnter="btnScore_MouseEnter" MouseLeave="btnScore_MouseLeave">

                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">

                                            <materialDesign:PackIcon Kind="ChartBoxOutline" Foreground="White" Name="MateriaScore" VerticalAlignment="Center" HorizontalAlignment="Left" Height="24" Width="24" Margin="0,0,0,0" ></materialDesign:PackIcon>
                                            <Label Name="lblScore" Content="{x:Static language:Lang.score}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="34" Width="240" Foreground="White" Margin="0,-4,1,0" Style="{DynamicResource Siemreap}" FontWeight="Bold"/>
                                        </Grid>
                                    </Button>

                                    <StackPanel Name="SettingPart" Margin="0 50 0 0">
                                        <Separator Foreground="Gray" VerticalAlignment="Bottom"></Separator>

                                        <Button Name="btnSetting" Height="35" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="5,0,5,5" Click="btnSetting_Click">
                                            <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">
                                                <materialDesign:PackIcon Name="MateriaSetting" Kind="Settings" VerticalAlignment="Center" HorizontalAlignment="Left" Height="24" Margin="-2,0,0,0" Width="28" ></materialDesign:PackIcon>
                                                <Label Name="lblSetting" FontWeight="Bold" Content="{x:Static language:Lang.setting}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="33" Width="246" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                                <materialDesign:PackIcon Name="MateriaSettingDrop" Kind="ChevronDown" VerticalAlignment="Center" HorizontalAlignment="Right" Height="24" Margin="0,0,15,0" Width="28" ></materialDesign:PackIcon>
                                                <materialDesign:PackIcon Name="MateriaSettingUp" Kind="ChevronUp" VerticalAlignment="Center" HorizontalAlignment="Right" Height="24" Margin="0,0,15,0" Width="28" ></materialDesign:PackIcon>

                                            </Grid>
                                        </Button>

                                        <Button Name="btnLang" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="50,0,5,10" Click="btnLang_Click">
                                            <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="222" Height="25">
                                                <materialDesign:PackIcon Name="MateriaLang" Kind="Language" VerticalAlignment="Center" HorizontalAlignment="Left" Height="20" Margin="0,0,0,0" Width="24" ></materialDesign:PackIcon>
                                                <Label Name="lblLang" FontWeight="Bold" Content="{x:Static language:Lang.language}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="12" VerticalContentAlignment="Center" Height="33" Width="205" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                                <materialDesign:PackIcon Name="MateriaLangDrop" Kind="ChevronDown" VerticalAlignment="Center" HorizontalAlignment="Right" Height="20" Margin="0,0,27,0" Width="24" ></materialDesign:PackIcon>
                                                <materialDesign:PackIcon Name="MateriaLangUp" Kind="ChevronUp" VerticalAlignment="Center" HorizontalAlignment="Right" Height="20" Margin="0,0,27,0" Width="24" ></materialDesign:PackIcon>

                                            </Grid>
                                        </Button>

                                        <Grid Height="30" Margin="85 -5 40 0" Name="gridLanguage">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="39"></ColumnDefinition>
                                                <ColumnDefinition Width="39"></ColumnDefinition>
                                                <ColumnDefinition Width="39"></ColumnDefinition>
                                                <ColumnDefinition Width="39"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Grid Grid.Column="0">
                                                <Button x:Name="Khmer" ToolTip="ខ្មែរ" Height="30" HorizontalAlignment="Stretch" Background="Transparent" BorderThickness="0" FontSize="15" Click="Khmer_Click">
                                                    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10" Height="30">
                                                        <Image  Name="Langkh" Source="/CamemisOffLine;component/Image/cambodia.png" Stretch="Fill" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="27" Margin="0,2,0,0" ></Image>

                                                    </Grid>
                                                </Button>
                                            </Grid>
                                            <Grid Grid.Column="1">
                                                <Button x:Name="English" ToolTip="English" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Click="English_Click">
                                                    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10" Height="30">
                                                        <Image  Name="LangEng" Source="/CamemisOffLine;component/Image/united-kingdom.png" Stretch="Fill" HorizontalAlignment="Left" VerticalAlignment="Center" Height="25" Width="27" ></Image>
                                                    </Grid>
                                                </Button>
                                            </Grid>
                                            <Grid Grid.Column="2">
                                                <Button Name="Chinese" ToolTip="Chinese" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Click="Chinese_Click">
                                                    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin=" -10 -12 -10 -10 " Height="25">
                                                        <Image  Name="LangChin" Source="/CamemisOffLine;component/Image/chinese.png" Stretch="Fill" HorizontalAlignment="Left" VerticalAlignment="Center" Height="17" Width="28" ></Image>
                                                    </Grid>
                                                </Button>
                                            </Grid>
                                            <Grid Grid.Column="3">
                                                <Button x:Name="Vietnam" ToolTip="Veitnamese"  Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Click="Vietnam_Click">
                                                    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10" Height="25">
                                                        <Image  Name="LangVit" Source="/CamemisOffLine;component/Image/vietnam.png" Stretch="Fill" HorizontalAlignment="Left" VerticalAlignment="Center" Height="25" Width="27" ></Image>

                                                    </Grid>
                                                </Button>
                                            </Grid>
                                        </Grid>

                                        <Button Name="btnColor" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="50,0,5,10">
                                            <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="222" Height="25">
                                                <materialDesign:PackIcon Name="MateriaColor" Kind="Color" VerticalAlignment="Center" HorizontalAlignment="Left" Height="20" Margin="0,0,0,0" Width="24" ></materialDesign:PackIcon>
                                                <Label Name="lblColor" FontWeight="Bold" Content="{x:Static language:Lang.color}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="12" VerticalContentAlignment="Center" Height="33" Width="205" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                                <materialDesign:PackIcon Name="MateriaColorDrop" Kind="ChevronDown" VerticalAlignment="Center" HorizontalAlignment="Right" Height="20" Margin="0,0,27,0" Width="24" ></materialDesign:PackIcon>
                                                <materialDesign:PackIcon Name="MateriaColorUp" Kind="ChevronUp" VerticalAlignment="Center" HorizontalAlignment="Right" Height="20" Margin="0,0,27,0" Width="24" ></materialDesign:PackIcon>

                                            </Grid>
                                        </Button>
                                        <Button Name="btnAbout" Height="30" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="50,0,5,10">
                                            <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="222" Height="25">
                                                <materialDesign:PackIcon Name="MateriaAbout" Kind="AboutOutline" VerticalAlignment="Center" HorizontalAlignment="Left" Height="20" Margin="0,0,0,0" Width="24" ></materialDesign:PackIcon>
                                                <Label Name="lblAbout" FontWeight="Bold" Content="{x:Static language:Lang.about}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="12" VerticalContentAlignment="Center" Height="33" Width="205" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                            </Grid>
                                        </Button>
                                        <Separator Foreground="Gray" VerticalAlignment="Bottom"></Separator>

                                        <Button Name="btnLogout" Height="35" HorizontalAlignment="Stretch"  Background="Transparent" BorderThickness="0" FontSize="15" Margin="5,0,5,5" Click="btnLogout_Click">
                                            <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="-10 0" Width="272" Height="25">
                                                <materialDesign:PackIcon Name="MateriaLogout" Kind="Logout" VerticalAlignment="Center" HorizontalAlignment="Left" Height="24" Margin="-2,0,0,0" Width="28" ></materialDesign:PackIcon>
                                                <Label Name="lblLogout" FontWeight="Bold" Content="{x:Static language:Lang.logout}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15" VerticalContentAlignment="Center" Height="33" Width="246" Foreground="White" Margin="0,-3,-6,0" Style="{DynamicResource Siemreap}"></Label>
                                            </Grid>
                                        </Button>
                                        <Separator Foreground="Gray" VerticalAlignment="Bottom"></Separator>

                                    </StackPanel>
                                </StackPanel>

                            </StackPanel>
                        </Grid>
                    </ScrollViewer>
                    <StackPanel Grid.Row="2" Margin="0 0 0 0">
                        <Grid>
                            <Label Name="lblnameCompany" Content="Power by CAMEMIS" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="15" FontFamily="Segoe UI Semibold"  Foreground="White">

                            </Label>
                        </Grid>
                    </StackPanel>
                </Grid>
            </DockPanel>

            <DockPanel DockPanel.Dock="Right">

                <Grid >
                    <Grid.RowDefinitions>

                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>


                    <Grid Grid.Row="1" Margin="0 0 0 0">
                        <Grid>
                            <TabControl BorderThickness="0" Margin=" 0 0 0 0" Name="tabcontrolMenu">
                                <TabItem BorderThickness="0"  Name="tabScore">
                                    <Grid Margin="-2 0 -2 -2" Background="White">
                                        <DockPanel LastChildFill="True">
                                            <DockPanel Name="DockTree" HorizontalAlignment="Left" Width="250" Margin="7 0 0 0" Background="WhiteSmoke">
                                                <Grid Margin="0 5 0 0">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="40"/>
                                                        <RowDefinition Height="*"/>
                                                    </Grid.RowDefinitions>
                                                    <Grid Grid.Row="0">
                                                        <Label Content="ឆ្នាំសិក្សា" Width="74" HorizontalAlignment="Left" FontSize="13" FontFamily="/CamemisOffLine;component/FontStyle/#Khmer OS Muol Light" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>

                                                    </Grid>
                                                    <Grid Grid.Row="1">
