                                                                                            <RowDefinition Height="50"/>
                                                                                            <RowDefinition Height="*"/>
                                                                                        </Grid.RowDefinitions>

                                                                                        <Grid Grid.Row="0">
                                                                                            <Grid.ColumnDefinitions>
                                                                                                <ColumnDefinition Width="420"/>
                                                                                                <ColumnDefinition Width="0"/>
                                                                                                <ColumnDefinition Width="1.2*"/>
